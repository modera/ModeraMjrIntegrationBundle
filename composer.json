{
    "name": "modera/mjr-integration-bundle",
    "type": "symfony-bundle",
    "description": "Provides low level integration for MJR.",
    "license": "MIT",
    "homepage": "http://modera.org",
    "keywords": ["mjr"],
    "authors": [
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^4.4|^5.0",
        "modera/direct-bundle": "^4.0",
        "sergeil/expander-bundle": "^1.2"
    },
    "require-dev": {
        "phake/phake": "^4.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": { "Modera\\MjrIntegrationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}
